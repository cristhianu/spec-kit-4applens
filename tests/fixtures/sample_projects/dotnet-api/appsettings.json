{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:myserver.database.windows.net,1433;Initial Catalog=mydatabase;Authentication=Active Directory Managed Identity;"
  },
  "Azure": {
    "StorageAccountName": "mystorageaccount",
    "KeyVaultName": "mykeyvault"
  },
  "Api": {
    "ApiKey": "your-api-key-here",
    "SecretToken": "your-secret-token"
  },
  "AllowedHosts": "*"
}
